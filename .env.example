APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

# PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

# Legacy database connection (for migration command)
# LEGACY_DB_HOST=127.0.0.1
# LEGACY_DB_PORT=3306
# LEGACY_DB_DATABASE=shopmata_legacy
# LEGACY_DB_USERNAME=root
# LEGACY_DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

# DigitalOcean Spaces
# DO_URL should be the region endpoint only (NOT including bucket name)
# Example: https://nyc3.digitaloceanspaces.com
# NOT: https://mybucket.nyc3.digitaloceanspaces.com
DO_SPACE_KEY=
DO_SPACE_SECRET=
DO_SPACE_BUCKET=
DO_SPACE_REGION=nyc3
DO_URL=https://nyc3.digitaloceanspaces.com
DO_CDN=https://your-bucket.nyc3.cdn.digitaloceanspaces.com

PORTAL_DOMAIN=portal.localhost

TWILIO_SID=
TWILIO_AUTH_TOKEN=
TWILIO_PHONE_NUMBER=

# Metals API (for precious metal spot prices)
# Sign up at https://metals-api.com
METALS_API_KEY=

# eBay Marketplace Integration
# Create an app at https://developer.ebay.com
EBAY_CLIENT_ID=
EBAY_CLIENT_SECRET=
EBAY_REDIRECT_URI="${APP_URL}/settings/marketplaces/ebay/callback"
EBAY_SANDBOX=true

# Amazon Marketplace Integration
# Apply for access at https://sellercentral.amazon.com/
AMAZON_CLIENT_ID=
AMAZON_CLIENT_SECRET=
AMAZON_REDIRECT_URI="${APP_URL}/settings/marketplaces/amazon/callback"
AMAZON_SANDBOX=true

# Etsy Marketplace Integration
# Create an app at https://www.etsy.com/developers
ETSY_CLIENT_ID=
ETSY_CLIENT_SECRET=
ETSY_REDIRECT_URI="${APP_URL}/settings/marketplaces/etsy/callback"

# Walmart Marketplace Integration
# Apply at https://developer.walmart.com/
WALMART_CLIENT_ID=
WALMART_CLIENT_SECRET=
WALMART_REDIRECT_URI="${APP_URL}/settings/marketplaces/walmart/callback"
WALMART_SANDBOX=true

VITE_APP_NAME="${APP_NAME}"

# Scout / Meilisearch
SCOUT_DRIVER=meilisearch
SCOUT_QUEUE=true
MEILISEARCH_HOST=http://127.0.0.1:7700
MEILISEARCH_KEY=
